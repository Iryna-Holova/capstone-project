{
  "extends": [
    "@parcel/config-default"
  ],
  "transformers": {
    "*.ts": [
      "@parcel/transformer-typescript-tsc"
    ],
    "*.scss": [
      "@parcel/transformer-sass"
    ],
    "*.svg": [
      "@parcel/transformer-raw"
    ]
  },
  "resolvers": [
    "@parcel/resolver-default"
  ]
}
